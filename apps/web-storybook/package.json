{
  "name": "web-storybook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev -p 3080\" \"npm run tailwind -- --watch\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind": "tailwindcss -i .storybook/src/tailwind.css -o .storybook/public/tailwind.compiled.css",
    "storybook": "concurrently \"yarn:watch:*\"",
    "build-storybook": "concurrently \"yarn:build:*\"",
    "build:css": "yarn tailwind",
    "build:storybook": "build-storybook",
    "watch:css": "yarn tailwind --watch",
    "watch:storybook": "start-storybook dev -p 6006"
  },
  "dependencies": {
    "next": "13.0.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ui-kit": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "18.11.12",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "concurrently": "^7.6.0",
    "eslint": "7.32.0",
    "eslint-config-application": "*",
    "eslint-plugin-storybook": "^0.6.10",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.2.4",
    "tsconfig-config-application": "*",
    "typescript": "4.9.4"
  }
}
