{
  "extends": "tsconfig-config-application/electron.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "commonjs",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "@application/*": [
        "src/application/*"
      ]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}