{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Electron",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noEmit": true,
    "incremental": true,
    "module": "esnext"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}